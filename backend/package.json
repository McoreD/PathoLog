{
  "name": "@patholog/backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:diff": "prisma migrate diff --from-empty --to-schema-datamodel prisma/schema.prisma --script > prisma/migrations/001_init.sql"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.694.0",
    "@aws-sdk/s3-request-presigner": "^3.694.0",
    "applicationinsights": "^2.9.6",
    "@prisma/client": "^5.14.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.4.0",
    "google-auth-library": "^9.6.3",
    "jose": "^5.2.1",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "pino": "^8.17.0",
    "pino-pretty": "^10.3.1",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.30",
    "@types/uuid": "^9.0.7",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "prisma": "^5.14.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5"
  }
}
